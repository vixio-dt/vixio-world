    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "^16.1.6",
    "jsdom": "^28.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
